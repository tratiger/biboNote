{
    "manifest_version": 3,
    "name": "biboNote",
    "version": "1.2",
    "description": "アイコンをクリックして、開いているページのタイトル, URL, 分類, 登録日時をオプションページに保存できます",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}